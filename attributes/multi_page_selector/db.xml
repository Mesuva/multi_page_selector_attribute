<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
    <table name="atMultiPageSelector">
        <field name="avID" type="integer" size="10">
            <unsigned/>
            <key/>
            <default value="0"/>
        </field>
        <field name="value" type="text">
            <default value="0"/>
            <notnull/>
        </field>
    </table>
    <table name="atMultiPageSelectorSettings">
        <field name="akID" type="integer" size="10">
            <unsigned/>
            <key/>
            <default value="0"/>
        </field>
        <field name="akParentID" type="integer">
            <unsigned/>
            <default value="1"/>
            <notnull/>
        </field>
        <field name="akPtID" type="integer">
            <unsigned/>
            <default value="0"/>
            <notnull/>
        </field>
        <field name="akRestrictSingle" type="boolean">
            <unsigned/>
            <default value="0"/>
            <notnull/>
        </field>
    </table>
</schema>